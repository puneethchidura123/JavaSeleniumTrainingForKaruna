package JavaOperators;

public class Assignment_Operators_Demo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int x =11,y=5;
		// +=
		System.out.println(x+=y);  //x =16,y=5
		//-=
		System.out.println(y-=x);  //x =16,y=-11  
		//*=
		System.out.println(x*=x);
	}

}
